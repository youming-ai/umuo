{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "bun"
    ],
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
