{
  "name": "yabaii-api",
  "version": "1.0.0",
  "description": "Backend API for Yabaii Japanese Price Comparison App",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bunx biome lint .",
    "lint:fix": "bunx biome lint --write .",
    "type-check": "bunx tsc --noEmit",
    "db:migrate": "bun run src/scripts/migrate.ts",
    "db:seed": "bun run src/scripts/seed.ts"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "@hono/node-server": "^1.8.0",
    "postgres": "^3.4.0",
    "redis": "^4.6.0",
    "zod": "^3.22.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "date-fns": "^3.0.0",
    "winston": "^3.11.0",
    "@sentry/bun": "^7.0.0",
    "openai": "^4.0.0",
    "ai": "^3.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/node": "^20.0.0",
    "biome": "^0.3.3",
    "typescript": "^5.3.0",
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "api",
    "bun",
    "typescript",
    "price-comparison",
    "japan",
    "hono"
  ],
  "author": "Yabaii Team",
  "license": "MIT"
}